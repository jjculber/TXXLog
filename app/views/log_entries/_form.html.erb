<%= form_with model: @log_entry, local: true do |form| %>
  <div class="form-group">
    <%= form.label :contact_at %>
    <%= form.datetime_local_field :contact_at, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :frequency %>
    <%= form.text_field :frequency, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :mode %>
    <%= form.text_field :mode, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :callsign %>
    <%= form.text_field :callsign, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :category %>
    <%= form.text_field :category, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :section %>
    <%= form.text_field :section, class: "form-control" %>
  </div>
  <%= form.submit "Submit" %>

<% end %>
