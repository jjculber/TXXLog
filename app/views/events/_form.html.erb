<%= form_with model: @event, local: true do |form| %>
  <div class="form-group">
    <%= form.label :event_name %>
    <%= form.text_field :event_name, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :callsign %>
    <%= form.text_field :callsign, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :category %>
    <%= form.text_field :category, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :section %>
    <%= form.text_field :section, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :exchange %>
    <%= form.text_field :exchange, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :start_time %>
    <%= form.datetime_local_field :start_time, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :end_time %>
    <%= form.datetime_local_field :end_time, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :location %>
    <%= form.text_field :location, class: "form-control" %>
  </div>
  <div class="form-group">
    <%= form.label :category_power %>
    <%= form.text_field :category_power, class: "form-control" %>
  </div>
  <%= form.submit "Submit" %>

<% end %>
